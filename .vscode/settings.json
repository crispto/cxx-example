{
  //global settting
  "workbench.colorTheme": "Monokai",
  "editor.wordWrap": "on",
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.formatOnSave": false,
  "explorer.confirmDelete": false,
  "terminal.integrated.defaultProfile.linux": "zsh",
  "window.titleBarStyle": "custom",
  //
  // go setting
  "go.testFlags": [
    "-v"
  ],
  "go.delveConfig": {
    "debugAdapter": "legacy",
  },
  "go.useLanguageServer": true,
  //
  // vue setting
  "vetur.format.defaultFormatter.html": "js-beautify-html",
  // "[html]": {
  //   "editor.defaultFormatter": "esbenp.prettier-vscode"
  // },
  // "[vue]": {
  //   "editor.defaultFormatter": "octref.vetur"
  // },
  // "editor.fontSize": 13,
  // "[javascript]": {
  //   "editor.defaultFormatter": "vscode.typescript-language-features"
  // },
  // "[jsonc]": {
  //   "editor.defaultFormatter": "vscode.json-language-features"
  // },
  // "[json]": {
  //   "editor.defaultFormatter": "vscode.json-language-features"
  // },
  // "workbench.editorAssociations": {
  //   "*.ipynb": "jupyter.notebook.ipynb"
  // },
  // "workbench.startupEditor": "newUntitledFile",
  // "gulp.autoDetect": "on",
  // "grunt.autoDetect": "on",
  // "jake.autoDetect": "on",
  "C_Cpp.errorSquiggles": "Disabled",
  "[c]": {
    "editor.defaultFormatter": "xaver.clang-format"
  },
  "C_Cpp.loggingLevel": "Debug",
  "[cpp]": {
    "editor.defaultFormatter": "xaver.clang-format"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.python",
  },
  "python.defaultInterpreterPath": "/usr/local/bin/python3",
  "python.formatting.provider": "autopep8",
  "python.linting.pylintEnabled": true,
  "python.linting.enabled": false,
  "python.languageServer": "Pylance",
  "files.associations": {
    "*.py": "python",
    "iomanip": "cpp",
    "iostream": "cpp",
    "cholesky": "cpp",
    "dense": "cpp",
    "ostream": "cpp",
    "deque": "cpp",
    "__locale": "cpp",
    "random": "cpp",
    "fstream": "cpp",
    "type_traits": "cpp",
    "*.ipp": "cpp",
    "cctype": "cpp",
    "clocale": "cpp",
    "cmath": "cpp",
    "csignal": "cpp",
    "cstdarg": "cpp",
    "cstddef": "cpp",
    "cstdio": "cpp",
    "cstdlib": "cpp",
    "cstring": "cpp",
    "ctime": "cpp",
    "cwchar": "cpp",
    "cwctype": "cpp",
    "any": "cpp",
    "array": "cpp",
    "atomic": "cpp",
    "strstream": "cpp",
    "*.tcc": "cpp",
    "bitset": "cpp",
    "chrono": "cpp",
    "codecvt": "cpp",
    "complex": "cpp",
    "condition_variable": "cpp",
    "cstdint": "cpp",
    "forward_list": "cpp",
    "list": "cpp",
    "unordered_map": "cpp",
    "vector": "cpp",
    "exception": "cpp",
    "algorithm": "cpp",
    "filesystem": "cpp",
    "functional": "cpp",
    "iterator": "cpp",
    "map": "cpp",
    "memory": "cpp",
    "memory_resource": "cpp",
    "numeric": "cpp",
    "optional": "cpp",
    "ratio": "cpp",
    "regex": "cpp",
    "set": "cpp",
    "string": "cpp",
    "string_view": "cpp",
    "system_error": "cpp",
    "tuple": "cpp",
    "utility": "cpp",
    "initializer_list": "cpp",
    "iosfwd": "cpp",
    "istream": "cpp",
    "limits": "cpp",
    "mutex": "cpp",
    "new": "cpp",
    "sstream": "cpp",
    "stdexcept": "cpp",
    "streambuf": "cpp",
    "thread": "cpp",
    "cfenv": "cpp",
    "cinttypes": "cpp",
    "typeindex": "cpp",
    "typeinfo": "cpp",
    "valarray": "cpp",
    "queue": "cpp"
  },
  "cmake.configureOnOpen": false
}