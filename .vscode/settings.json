{
  //global settting
  "workbench.colorTheme": "Monokai",
  "editor.wordWrap": "on",
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.formatOnSave": true,
  //
  // go setting
  "go.testFlags": [
    "-v"
  ],
  "go.delveConfig": {
    "debugAdapter": "legacy",
  },
  "go.useLanguageServer": true,
  //
  // vue setting
  "vetur.format.defaultFormatter.html": "js-beautify-html",
  // "[html]": {
  //   "editor.defaultFormatter": "esbenp.prettier-vscode"
  // },
  // "[vue]": {
  //   "editor.defaultFormatter": "octref.vetur"
  // },
  // "editor.fontSize": 13,
  // "[javascript]": {
  //   "editor.defaultFormatter": "vscode.typescript-language-features"
  // },
  // "[jsonc]": {
  //   "editor.defaultFormatter": "vscode.json-language-features"
  // },
  // "[json]": {
  //   "editor.defaultFormatter": "vscode.json-language-features"
  // },
  // "workbench.editorAssociations": {
  //   "*.ipynb": "jupyter.notebook.ipynb"
  // },
  // "workbench.startupEditor": "newUntitledFile",
  // "gulp.autoDetect": "on",
  // "grunt.autoDetect": "on",
  // "jake.autoDetect": "on",
  "explorer.confirmDelete": true,
  "C_Cpp.errorSquiggles": "Disabled",
  "[c]": {
    "editor.defaultFormatter": "xaver.clang-format"
  },
  "C_Cpp.loggingLevel": "Debug",
  "[cpp]": {
    "editor.defaultFormatter": "xaver.clang-format"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.python",
  },
  "python.defaultInterpreterPath": "/usr/local/bin/python3",
  "python.formatting.provider": "autopep8",
  "files.associations": {
    "random": "cpp",
    "cmath": "cpp",
    "__config": "cpp",
    "complex": "cpp",
    "chrono": "cpp",
    "__bit_reference": "cpp",
    "__debug": "cpp",
    "__functional_base": "cpp",
    "__hash_table": "cpp",
    "__locale": "cpp",
    "__mutex_base": "cpp",
    "__nullptr": "cpp",
    "__split_buffer": "cpp",
    "__string": "cpp",
    "__threading_support": "cpp",
    "__tree": "cpp",
    "__tuple": "cpp",
    "algorithm": "cpp",
    "array": "cpp",
    "atomic": "cpp",
    "bitset": "cpp",
    "cctype": "cpp",
    "clocale": "cpp",
    "condition_variable": "cpp",
    "cstdarg": "cpp",
    "cstddef": "cpp",
    "cstdint": "cpp",
    "cstdio": "cpp",
    "cstdlib": "cpp",
    "cstring": "cpp",
    "ctime": "cpp",
    "cwchar": "cpp",
    "cwctype": "cpp",
    "deque": "cpp",
    "exception": "cpp",
    "optional": "cpp",
    "string_view": "cpp",
    "fstream": "cpp",
    "functional": "cpp",
    "initializer_list": "cpp",
    "iomanip": "cpp",
    "ios": "cpp",
    "iosfwd": "cpp",
    "iostream": "cpp",
    "istream": "cpp",
    "iterator": "cpp",
    "limits": "cpp",
    "list": "cpp",
    "locale": "cpp",
    "map": "cpp",
    "memory": "cpp",
    "mutex": "cpp",
    "new": "cpp",
    "numeric": "cpp",
    "ostream": "cpp",
    "queue": "cpp",
    "ratio": "cpp",
    "sstream": "cpp",
    "stdexcept": "cpp",
    "streambuf": "cpp",
    "string": "cpp",
    "system_error": "cpp",
    "thread": "cpp",
    "tuple": "cpp",
    "type_traits": "cpp",
    "typeinfo": "cpp",
    "unordered_map": "cpp",
    "utility": "cpp",
    "vector": "cpp",
    "*.tcc": "cpp",
    "hash_map": "cpp",
    "cinttypes": "cpp",
    "hashtable": "cpp"
  },
  "python.linting.pylintEnabled": true,
  "python.linting.enabled": true,
  "python.languageServer": "Pylance"

}
